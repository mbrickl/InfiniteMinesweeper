{"ts":1346455844170,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"#Game time in seconds\nMAX_TIME=300\n\nclass Game\n    constructor: ->\n        @field = new Field\n        @score = 0\n        @inProgress = false\n        @time = MAX_TIME\n\n    updateScore: (increment) ->\n        @score += increment\n        $('score').text=@score\n\n    runClock: ->\n        if this.inProgress\n            @time--\n            $('clock').text=@time/60 + \":\" + @time%60\n            setTimeout runClock,1000\n\n    startClock: ->\n        if not @inProgress\n            @inProgress = true\n            setTimeout runClock,1000\n            \n    stopClock: ->\n        @inProgress = false\n            \n\n        "]],"start1":0,"start2":0,"length1":0,"length2":610}]],"length":610}
